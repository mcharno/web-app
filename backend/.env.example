# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=charno_web
DB_USER=postgres
DB_PASSWORD=postgres

# Mock Mode Configuration
# Set to 'true' to use mock data instead of real database
# Perfect for local development without setting up PostgreSQL
USE_MOCK_DB=false

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

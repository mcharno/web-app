{
  "transform": {},
  "testEnvironment": "node",
  "coverageDirectory": "coverage",
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/server.js",
    "!src/config/database.js",
    "!src/mocks/**",
    "!src/__tests__/**"
  ],
  "testMatch": [
    "**/__tests__/**/*.test.js",
    "**/?(*.)+(spec|test).js"
  ],
  "moduleFileExtensions": ["js"],
  "verbose": true
}
